<Window x:Class="StockP1.stock"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockP1"
        mc:Ignorable="d"
        Title="MainWindow" Height="322.549" Width="620.098">
	<Grid>
		<DataGrid AutoGenerateColumns="False" Height="272" HorizontalAlignment="Left" Margin="10,10,0,0" Name="dataS" VerticalAlignment="Top" Width="350" ItemsSource="{Binding}">
			<DataGrid.Columns>
				<DataGridTextColumn Header="Cantidad" Binding="{Binding Path=quantity}"/>
				<DataGridTextColumn Header="Descripcion" Binding="{Binding Path=description}"/>
				<DataGridTextColumn Header="Stock" Binding="{Binding Path=Stock}"/>
			</DataGrid.Columns>
		</DataGrid>

	</Grid>
</Window>
