<Window x:Class="StockP1.user"
     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockP1"
        mc:Ignorable="d"
        Title="user" Height="300" Width="550">
	<Grid>
		<DataGrid AutoGenerateColumns="False" Height="249" HorizontalAlignment="Left" Margin="10,10,0,0" Name="dataU" VerticalAlignment="Top" Width="350" ItemsSource="{Binding}" SelectionChanged="dataU_SelectionChanged">
			<DataGrid.Columns>
				<DataGridTextColumn Header="Usuario" Binding="{Binding Path=User}"/>
				<DataGridTextColumn Header="Descripcion" Binding="{Binding Path=descriptionU}"/>
				<DataGridTextColumn Header="Cantidad Usuario" Binding="{Binding Path=quantityU}"/>
			</DataGrid.Columns>
		</DataGrid>
	</Grid>
</Window>
